{
   "content_scripts": [{
        "js": [ "index.js" ],
        "matches": [ "https://*.twitter.com/*" ],
        "run_at": "document_idle"
   }],
   "description": "Gallery view for a Twitter user's media.",
    "icons": {
      "48": "images/twitter_gallery.png"
   },
   "manifest_version": 2,
   "name": "Twitter Gallery",
   "offline_enabled": true,
   "permissions": [ "https://*.twitter.com/*" ],
   "short_name": "Twitter Gallery",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.1"
}
